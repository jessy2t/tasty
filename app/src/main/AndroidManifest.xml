<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.acme.tasty">
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.Tasty">
        <activity
                android:name=".RestaurantRegistration2"
                android:exported="false" />
        <activity
                android:name=".ui.login.RestaurantLoginActivity"
                android:exported="true"
                android:label="@string/title_activity_restaurant_login"
                android:parentActivityName=".MainActivity"/>
        <activity
                android:name=".CustomerRestaurantOverviewActivity"
                android:exported="true"
                android:label="@string/title_activity_customer_restaurantOverview"
                android:parentActivityName=".CustomerRecommendationActivity"/>
        <activity
                android:name=".CustomerRecommendationActivity"
                android:exported="true"
                android:label="@string/title_activity_customer_recommendation"
                android:parentActivityName=".CustomerSwipeActivity"/>
        <activity
                android:name=".CustomerSwipeActivity"
                android:exported="true"
                android:label="@string/title_activity_customer_swipe"
                android:parentActivityName=".CustomerStartActivity"/>
        <activity
                android:name=".RestaurantRegistrationActivity"
                android:exported="true"
                android:label="@string/title_activity_restaurant_registration"
                android:parentActivityName=".ui.login.RestaurantLoginActivity"/>
        <activity
                android:name=".CustomerStartActivity"
                android:exported="true"
                android:label="@string/title_activity_customer_start"
                android:parentActivityName=".MainActivity"/>
        <activity
                android:name=".CustomerPreferencesActivity"
                android:exported="true"
                android:label="@string/title_activity_customer_start"
                android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".CustomerPreferencesCategoryActivity"
            android:exported="true"
            android:label="@string/title_activity_customer_start"
            android:parentActivityName=".CustomerPreferencesActivity"/>
        <activity
            android:name=".CustomerPreferencesMealtypeActivity"
            android:exported="true"
            android:label="@string/title_activity_customer_start"
            android:parentActivityName=".CustomerPreferencesActivity"/>
        <activity
                android:name=".MainActivity"
                android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>
</manifest>